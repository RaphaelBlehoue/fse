{% extends 'base.html.twig' %}
{% block title %}
    {{ 'L\'Ecosystème la finance s\'engage' }}
{% endblock %}
{% block slide %}
    {{ include('front/includes/slide.html.twig') }}
{% endblock %}
{% block body %}
    <!-- BEGIN: CONTENT/ORGANISATEUR -->
    <div class="c-content-box c-size-md c-bg-white">
        <div class="container">
            <div class="c-content-box c-size-md">
                <div class="container">
                    <div class="c-content-title-1 wow animated fadeIn">
                        <h3 class="c-center c-font-uppercase c-font-bold">{{ 'les organisateurs'|upper }}</h3>
                        <div class="c-line-center c-theme-bg"></div>
                    </div>
                </div>
            </div>
            {% for organizer in organizers %}
                {% if loop.first %}
                    <div class="c-content-box c-overflow-hide">
                        <div class="c-content-product-3 c-bs-grid-reset-space">
                            <div class="row">
                                <div class="col-md-6 col-sm-6">
                                    <div class="c-content-overlay">
                                        <div class="c-bg-img-center c-overlay-object" data-height="height" style="height: 350px; width: 95%; background-image: url('{{ asset(organizer.assertPath|imagine_filter('thumb_medium'))}}');"></div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-6">
                                    <div class="c-wrapper c-bg-green-1" style="height: 350px;">
                                        <div class="c-content c-border c-border-padding c-border-padding-right">
                                            <h3 class="c-title c-font-25 c-font-white c-font-uppercase c-font-bold">{{ organizer.name }}</h3>
                                            <p class="c-description c-font-17 c-font-white">{{ organizer.content|striptags|truncate(150, false, '...') }}</p>
                                            <a href="{{ organizer.website }}" target="_blank" class="btn btn-lg c-btn-white c-font-uppercase c-btn-square c-btn-border-1x">{{ 'en savoir plus'|upper }}</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {% else %}
                    <div class="c-content-box c-size-lg c-no-padding c-overflow-hide c-bg-white">
                        <div class="c-content-product-3 c-bs-grid-reset-space">
                            <div class="row">
                                <div class="col-md-6 col-sm-6">
                                    <div class="c-wrapper c-theme-bg" style="height: 350px;">
                                        <div class="c-content c-border c-border-padding c-border-padding-left">
                                            <h3 class="c-title c-font-25 c-font-white c-font-uppercase c-font-bold">{{ organizer.name }}</h3>
                                            <p class="c-description c-font-17 c-font-white">{{ organizer.content|striptags|truncate(150, false, '...') }}</p>
                                            <a href="{{ organizer.website }}" target="_blank" class="btn btn-lg c-btn-white c-font-uppercase c-btn-square c-btn-border-1x">{{ 'en savoir plus'|upper }}</a>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6 col-sm-6">
                                    <div class="c-content-overlay">
                                        <div class="c-bg-img-center c-overlay-object" data-height="height" style="height: 350px; width: 95%; background-image: url('{{ asset(organizer.assertPath|imagine_filter('thumb_medium'))}}');"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                {% endif %}
            {% endfor %}
        </div>
    </div>
    <!-- END: CONTENT/ORGANISATEUR -->

