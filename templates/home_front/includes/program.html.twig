            <span class="c-font-16">
                {{ part.title }}
            </span>
        </h5>
        <div class="c-line-center c-bg-green-1"></div>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>{{ 'Horaires' }}</th>
                <th>{{ 'DÃ©roulement' }}</th>
            </tr>
        </thead>
        <tbody>
        {% for program in  part.programs %}
            {% if program.section.id == sectionId %}
                <tr {% if loop.index is odd %} class="active" {% endif %}>
