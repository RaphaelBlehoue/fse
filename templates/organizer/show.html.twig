{% extends 'fse/fse_layout.html.twig' %}

{% block title %} {{ 'Organisateur' }} {% endblock %}
{% block fse_title %} {{ '| Organisateur'|upper }}{% endblock %}
        {% block fse_content %}
            <div class="col-6">
                <div class="card">
                    <header class="card-header">
                        <h4 class="card-title">{{ 'Informations Intervenant'|upper }}</h4>
                        {{ include('organizer/_delete_form.html.twig') }}
                    </header>
                    <img class="card-img-top" src="{{ asset(intervenant.assertPath) }}" alt="Card image cap">
                    <div class="card-body">
                        <table class="table">
                            <tbody>
                            <tr>
                                <th>Id</th>
                                <td>{{ organizer.id }}</td>
                            </tr>
                            <tr>
                                <th>ImageSize</th>
                                <td>{{ organizer.imageSize }}</td>
                            </tr>
                            <tr>
                                <th>ImageName</th>
                                <td>{{ organizer.imageName }}</td>
                            </tr>
                            <tr>
                                <th>Name</th>
                                <td>{{ organizer.name }}</td>
                            </tr>
                            <tr>
                                <th>Website</th>
                                <td>{{ organizer.website }}</td>
                            </tr>
                            <tr>
                                <th>Content</th>
                                <td>{{ organizer.content }}</td>
                            </tr>
                            <tr>
                                <th>Created</th>
                                <td>{{ organizer.created ? organizer.created|date('Y-m-d H:i:s') : '' }}</td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="card-footer">
                        <a class="btn btn-label btn-dark" href="{{ path('organizer_index') }}">
                            <label><i class="ti-arrow-left"></i></label>
                            {{ 'Retour Ã  la liste'}}
                        </a>
                        <a class="btn btn-label btn-danger" href="{{ path('organizer_edit', {'id': organizer.id}) }}">
                            <label><i class="ti-pencil-alt"></i></label>
                            {{ 'Modifier' }}
                        </a>
                    </div>
                </div>
            </div>
        {% endblock %}
